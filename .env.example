# Required: AO Compute Unit URL for dryrun calls
CU_URL=https://cu.ao-testnet.xyz

# Optional: Hyperbeam API base URL (default: https://compute.hyperbeam.xyz)
HYPERBEAM_BASE_URL=https://compute.hyperbeam.xyz

# Optional: Number of concurrent API requests (default: 15, range: 1-100)
CONCURRENCY=15

# Optional: Number of retry attempts for failed requests (default: 3, range: 0-10)
RETRY_ATTEMPTS=3

# Optional: Initial retry delay in milliseconds (default: 1000)
RETRY_DELAY_MS=1000

# Optional: Request timeout in milliseconds (default: 30000)
TIMEOUT=30000

# Optional: Maximum number of addresses to check (default: all addresses)
MAX_ADDRESSES=10

# ============================================================================
# CU Comparison Tool Configuration (cu-compare)
# ============================================================================

# Optional: First CU URL for comparison (default: https://cu.ardrive.io)
CU_URL_A=https://cu.ardrive.io

# Optional: Second CU URL for comparison (default: https://cu.ao-testnet.xyz)
CU_URL_B=https://cu.ao-testnet.xyz

# Optional: Path to wallet JWK file for signing messages (default: ./demo.json)
WALLET_PATH=./demo.json
