{
  "name": "balance-checker",
  "version": "1.0.0",
  "description": "CLI tool to validate AO process balances against Hyperbeam API",
  "main": "dist/cli.js",
  "bin": {
    "balance-checker": "./dist/cli.js",
    "cu-compare": "./dist/cu-compare.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "dev:cu": "tsx src/cu-compare.ts",
    "start": "node dist/cli.js",
    "start:cu": "node dist/cu-compare.js",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "ao",
    "balance",
    "checker",
    "cli",
    "hyperbeam",
    "permaweb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@permaweb/aoconnect": "^0.0.59",
    "axios": "^1.7.7",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "p-limit": "^3.1.0",
    "cli-progress": "^3.12.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
